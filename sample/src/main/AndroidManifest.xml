<manifest package="com.tutorials.android.particles.sample"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity android:name="com.tutorials.android.particles.sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.tutorials.android.particles.sample.FallingParticlesFromTopActivity"
            android:parentActivityName="com.tutorials.android.particles.sample.MainActivity" />
        <activity android:name="com.tutorials.android.particles.sample.FallingParticlesFromPointActivity"
            android:parentActivityName="com.tutorials.android.particles.sample.MainActivity" />
        <activity android:name="com.tutorials.android.particles.sample.ExplosionActivity"
            android:parentActivityName="com.tutorials.android.particles.sample.MainActivity" />
        <activity android:name="com.tutorials.android.particles.sample.ShimmeringActivity"
            android:parentActivityName="com.tutorials.android.particles.sample.MainActivity" />
        <activity android:name="com.tutorials.android.particles.sample.FallingWithTouchActivity"
            android:parentActivityName="com.tutorials.android.particles.sample.MainActivity" />
        <activity android:name="com.tutorials.android.particles.sample.FallingParticlesWithListenerActivity"
            android:parentActivityName="com.tutorials.android.particles.sample.MainActivity" />

    </application>

</manifest>
